{
  "expo": {
    "name": "Pok√©deck Builder",
    "slug": "pokemon-deck-builder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.silviamalavasi.pokedeckbuilder",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.silviamalavasi.pokedeckbuilder",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#8249F2"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-combined.png",
          "resizeMode": "cover",
          "backgroundColor": "#4630EB",
          "dark": {
            "image": "./assets/images/splash-combined.png",
            "resizeMode": "cover",
            "backgroundColor": "#4630EB"
          }
        }
      ],
      "expo-sqlite",
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5c5ced7b-0261-4a0a-808d-3477e4c6af0e"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/5c5ced7b-0261-4a0a-808d-3477e4c6af0e"
    },
    "splash": {
      "image": "./assets/images/splash-combined.png",
      "resizeMode": "cover",
      "backgroundColor": "#4630EB"
    }
  }
}
